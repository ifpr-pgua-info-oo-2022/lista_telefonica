<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400" prefWidth="600" spacing="5.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ifpr.pgua.eic.listatelefonica.controllers.JanelaCadastro">

    <Label text="Nome:" />
    <TextField fx:id="tfNome" />

    <Label text="Email:" />
    <TextField fx:id="tfEmail" />

    <Label text="Telefone:" />
    <TextField fx:id="tfTelefone" />

    <HBox spacing="5.0">
        <Button onAction="#cadastrar" text="Cadastrar" />
        <Button onAction="#voltar" text="Voltar" />
    </HBox>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>

</VBox>
